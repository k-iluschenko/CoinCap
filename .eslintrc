{
  "parser": "babel-eslint",
  "plugins": ["react", "import", "jsx-a11y", "fetch"],
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },

  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
		"react/jsx-uses-react": "error",
		"react/jsx-uses-vars": "error"
  }
}
